{
  "Application": {
    "DisplayName": "Agent SDK Demo",
    "AgentName": "Agent",
    "AgentNameFormat": "Agent-{0}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "MicrosoftAgentSDKDemo": "Information",
      "MicrosoftAgentSDKDemo.Services.CosmosDbChatMessageStore": "Information",
      "MicrosoftAgentSDKDemo.Services.CosmosDbAgentThreadStore": "Warning",
      "Microsoft": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "your-app-insights-key-here"
  },
  "AzureOpenAI": {
    "Endpoint": "https://your-openai-resource.cognitiveservices.azure.com",
    "DeploymentName": "gpt-4o",
    "DallEEndpoint": "https://your-dalle-resource.cognitiveservices.azure.com",
    "DallEDeploymentName": "dall-e-3",
    "SystemInstructionsFile": "prompts/system-instructions.txt"
  },
  "CosmosDB": {
    "Endpoint": "https://your-cosmos-account.documents.azure.com:443/",
    "DatabaseName": "agent-database",
    "ContainerId": "conversations"
  },
  "MCPServers": {
    "Servers": [
      {
        "Name": "Microsoft Learn",
        "Endpoint": "https://learn.microsoft.com/api/mcp",
        "Enabled": true,
        "TimeoutSeconds": 30
      }
    ]
  }
}
